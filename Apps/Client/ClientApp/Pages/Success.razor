<MudDialog>
    <DialogContent>
        Заказ успешно сформирован!
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">На домашнюю страницу</MudButton>
        <MudButton Color="Color.Success" OnClick="Submit">Заказы</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }
    void Cancel()
    {
        MudDialog.Close(DialogResult.Cancel());
    }
}